{
    "dashboard": {
        "id": null,
        "uid": "evaonline-user-dashboard",
        "title": "EVAonline - Vis√£o do Usu√°rio",
        "description": "Dashboard simplificado com m√©tricas de neg√≥cio para usu√°rios finais",
        "tags": [
            "evaonline",
            "user-friendly",
            "business-metrics"
        ],
        "timezone": "browser",
        "schemaVersion": 38,
        "version": 1,
        "refresh": "30s",
        "panels": [
            {
                "id": 1,
                "title": "üü¢ Status do Sistema",
                "type": "stat",
                "targets": [
                    {
                        "expr": "up{job='evaonline-api'}",
                        "refId": "A"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "mappings": [
                            {
                                "type": "value",
                                "options": {
                                    "0": {
                                        "text": "‚ùå Offline",
                                        "color": "red"
                                    },
                                    "1": {
                                        "text": "‚úÖ Online",
                                        "color": "green"
                                    }
                                }
                            }
                        ],
                        "thresholds": {
                            "mode": "absolute",
                            "steps": [
                                {
                                    "value": 0,
                                    "color": "red"
                                },
                                {
                                    "value": 1,
                                    "color": "green"
                                }
                            ]
                        },
                        "unit": "none"
                    }
                },
                "gridPos": {
                    "h": 4,
                    "w": 6,
                    "x": 0,
                    "y": 0
                }
            },
            {
                "id": 2,
                "title": "‚è±Ô∏è Response Time (Blackbox)",
                "type": "stat",
                "targets": [
                    {
                        "expr": "probe_duration_seconds{job='evaonline-blackbox'}",
                        "refId": "A"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "unit": "s",
                        "decimals": 2,
                        "thresholds": {
                            "mode": "absolute",
                            "steps": [
                                {
                                    "value": 0,
                                    "color": "green"
                                },
                                {
                                    "value": 1,
                                    "color": "yellow"
                                },
                                {
                                    "value": 2,
                                    "color": "red"
                                }
                            ]
                        }
                    }
                },
                "gridPos": {
                    "h": 4,
                    "w": 6,
                    "x": 6,
                    "y": 0
                }
            },
            {
                "id": 3,
                "title": "üìä Health Checks (24h)",
                "type": "stat",
                "targets": [
                    {
                        "expr": "sum(increase(probe_success{job='evaonline-blackbox'}[24h]))",
                        "refId": "A"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "unit": "short",
                        "decimals": 0,
                        "color": {
                            "mode": "thresholds"
                        },
                        "thresholds": {
                            "mode": "absolute",
                            "steps": [
                                {
                                    "value": 0,
                                    "color": "blue"
                                }
                            ]
                        }
                    }
                },
                "gridPos": {
                    "h": 4,
                    "w": 6,
                    "x": 12,
                    "y": 0
                }
            },
            {
                "id": 4,
                "title": "‚ö° Redis Hit Rate (%)",
                "type": "gauge",
                "targets": [
                    {
                        "expr": "redis_keyspace_hits_total / (redis_keyspace_hits_total + redis_keyspace_misses_total) * 100",
                        "refId": "A"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "unit": "percent",
                        "min": 0,
                        "max": 100,
                        "thresholds": {
                            "mode": "absolute",
                            "steps": [
                                {
                                    "value": 0,
                                    "color": "red"
                                },
                                {
                                    "value": 50,
                                    "color": "yellow"
                                },
                                {
                                    "value": 80,
                                    "color": "green"
                                }
                            ]
                        }
                    }
                },
                "gridPos": {
                    "h": 4,
                    "w": 6,
                    "x": 18,
                    "y": 0
                }
            },
            {
                "id": 5,
                "title": "üìà Health Checks por Hora",
                "type": "graph",
                "targets": [
                    {
                        "expr": "rate(probe_success{job='evaonline-blackbox'}[1h])",
                        "legendFormat": "Checks/hora",
                        "refId": "A"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "unit": "reqps"
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 0,
                    "y": 4
                }
            },
            {
                "id": 6,
                "title": "üíæ Redis Operations",
                "type": "graph",
                "targets": [
                    {
                        "expr": "rate(redis_commands_total[5m])",
                        "legendFormat": "Redis Commands",
                        "refId": "A"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "unit": "ops"
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 12,
                    "y": 4
                }
            },
            {
                "id": 7,
                "title": "üåç Service Availability",
                "type": "stat",
                "targets": [
                    {
                        "expr": "up",
                        "legendFormat": "{{job}} - {{instance}}",
                        "refId": "A"
                    }
                ],
                "gridPos": {
                    "h": 8,
                    "w": 8,
                    "x": 0,
                    "y": 12
                }
            },
            {
                "id": 8,
                "title": "‚ö†Ô∏è HTTP Probe Failure Rate (%)",
                "type": "stat",
                "targets": [
                    {
                        "expr": "(1 - probe_success{job='evaonline-blackbox'}) * 100",
                        "refId": "A"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "unit": "percent",
                        "decimals": 2,
                        "thresholds": {
                            "mode": "absolute",
                            "steps": [
                                {
                                    "value": 0,
                                    "color": "green"
                                },
                                {
                                    "value": 1,
                                    "color": "yellow"
                                },
                                {
                                    "value": 5,
                                    "color": "red"
                                }
                            ]
                        }
                    }
                },
                "gridPos": {
                    "h": 4,
                    "w": 8,
                    "x": 8,
                    "y": 12
                }
            },
            {
                "id": 9,
                "title": "üïê Uptime das √öltimas 24h (%)",
                "type": "stat",
                "targets": [
                    {
                        "expr": "(avg_over_time(up{job='evaonline-api'}[24h])) * 100",
                        "refId": "A"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "unit": "percent",
                        "decimals": 2,
                        "thresholds": {
                            "mode": "absolute",
                            "steps": [
                                {
                                    "value": 0,
                                    "color": "red"
                                },
                                {
                                    "value": 95,
                                    "color": "yellow"
                                },
                                {
                                    "value": 99,
                                    "color": "green"
                                }
                            ]
                        }
                    }
                },
                "gridPos": {
                    "h": 4,
                    "w": 8,
                    "x": 16,
                    "y": 12
                }
            },
            {
                "id": 10,
                "title": "üöÄ Service Health Overview",
                "type": "table",
                "targets": [
                    {
                        "expr": "up",
                        "refId": "A",
                        "format": "table"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "custom": {
                            "align": "auto",
                            "displayMode": "auto"
                        }
                    },
                    "overrides": [
                        {
                            "matcher": {
                                "id": "byName",
                                "options": "Value"
                            },
                            "properties": [
                                {
                                    "id": "displayName",
                                    "value": "Total de Requests"
                                },
                                {
                                    "id": "unit",
                                    "value": "short"
                                },
                                {
                                    "id": "decimals",
                                    "value": 0
                                }
                            ]
                        }
                    ]
                },
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 0,
                    "y": 16
                }
            },
            {
                "id": 11,
                "title": "üí° Redis Performance",
                "type": "stat",
                "targets": [
                    {
                        "expr": "rate(redis_commands_total[24h])",
                        "refId": "A"
                    }
                ],
                "description": "Total de opera√ß√µes Redis nas √∫ltimas 24h",
                "fieldConfig": {
                    "defaults": {
                        "unit": "s",
                        "decimals": 0,
                        "color": {
                            "mode": "thresholds"
                        },
                        "thresholds": {
                            "mode": "absolute",
                            "steps": [
                                {
                                    "value": 0,
                                    "color": "blue"
                                }
                            ]
                        }
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 12,
                    "y": 16
                }
            }
        ],
        "templating": {
            "list": []
        },
        "annotations": {
            "list": [
                {
                    "builtIn": 1,
                    "datasource": "-- Grafana --",
                    "enable": true,
                    "hide": true,
                    "iconColor": "rgba(0, 211, 255, 1)",
                    "name": "Annotations & Alerts",
                    "type": "dashboard"
                }
            ]
        },
        "time": {
            "from": "now-6h",
            "to": "now"
        },
        "timepicker": {
            "refresh_intervals": [
                "30s",
                "1m",
                "5m",
                "15m",
                "30m",
                "1h"
            ]
        }
    }
}
